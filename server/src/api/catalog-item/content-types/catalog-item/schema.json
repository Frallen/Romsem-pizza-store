{
  "kind": "collectionType",
  "collectionName": "catalog_items",
  "info": {
    "singularName": "catalog-item",
    "pluralName": "catalog-items",
    "displayName": "catalogItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "itemID": {
      "type": "uid",
      "required": true
    },
    "Text": {
      "type": "text"
    },
    "Title": {
      "type": "string",
      "required": true
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "Active": {
      "type": "boolean",
      "required": true
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Пицца",
        "Сеты",
        "Wok",
        "Роллы",
        "Суши",
        "Салаты",
        "Супы",
        "Корн доги",
        "Напитки"
      ],
      "required": true
    },
    "main_slider": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::main-slider.main-slider",
      "mappedBy": "catalog_item"
    }
  }
}

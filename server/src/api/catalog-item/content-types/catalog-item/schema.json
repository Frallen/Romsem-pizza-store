{
  "kind": "collectionType",
  "collectionName": "catalog_items",
  "info": {
    "singularName": "catalog-item",
    "pluralName": "catalog-items",
    "displayName": "catalogItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "itemID": {
      "type": "uid",
      "required": true
    },
    "Text": {
      "type": "text"
    },
    "Title": {
      "type": "string",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "Active": {
      "type": "boolean",
      "required": true
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Пицца",
        "Сеты",
        "Wok",
        "Роллы",
        "Суши",
        "Салаты",
        "Супы",
        "Корндоги",
        "Напитки"
      ],
      "required": true
    },
    "main_slider": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::main-slider.main-slider",
      "mappedBy": "catalog_item"
    },
    "product_sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::prioduct-size.prioduct-size"
    },
    "DetailImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "Description": {
      "type": "string",
      "required": false
    },
    "ingridients": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ingidient-by-one.ingidient-by-one"
    }
  }
}
